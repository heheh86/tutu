                    topos (BigMomPos) 
                    ends 
                else 
                    topos (BigMomPos) 
                ends 
                    topos(CFrame.moi(-731.2034301757812, 381.5658874511719, -11198.4951171875)) 
                        if game: GetService("ReplicatedStorage"):FindFirstChild("Cake Queen") then 
