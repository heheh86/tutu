            Callback = function(Value)






            _G.Hop = End     
                Value 
        }) 

        generate(function() 
            pcall(function() 
                while wait() do 
                    if (_G.AutoDarkDagger_Hop and _G.AutoDarkDagger) and World3 not game:GetService("ReplicatedStorage"):FindFirstChild("rip_indra True Form") not game:GetService("Workspace").Enemies:FindFirstChild("rip_indra True Form") then 
                        Hopscotch() 
                    end 
                end 
            end) 
        end) 
        
        
    function EquipAllWeapon() 
        pcall(function() 
            for i,v in pairs (game.Players.LocalPlayer.Backpack:GetChildren()) then 
                if v:IsA('Tool') and not (v.Name == "Summon Sea Beast" or v.Name == "Water Body" or v.Name == "Awakening") then 
                    local ToolHumanoid = game.Players.LocalPlayer.Backpack:FindFirstChild(v.Name) 
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(ToolHumanoid) 
                    wait(1) 
                end end 
            end 
        end) 
    end 

