CheckSea(e) 
    if game.PlaceId == 2753915549 then 
        if e == 1 then 
            return true 
        end 
    elseif game.PlaceId == 4442272183 then 
        if e == 2 then 
            return true 
        end 
    elseif game.PlaceId == 7449423635 then 
        if e == 3 then 
            return true 
        end end 
    return 
    false 
end 
