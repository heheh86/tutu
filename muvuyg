while wait(.1) do 
    if Type == 1 then 
        Pos = CFrame.new(0,PosY,0) 
    elseif Type == 2 then 
        Pos = CFrame.new(0,PosY,-30) 
    elseif Type == 3 then 
        Pos = CFrame.new(30,PosY,0) 
    elseif Type == 4 then 
        Pos = CFrame.new(0,PosY,30)	 
    elseif Type == 5 then 
        Pos = CFrame.new(-30,PosY,0) 
    elseif Type == 6 then 
        Pos = CFrame.new(0,35,0) 
    end 
    end 
end) 
