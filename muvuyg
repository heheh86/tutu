sinh ra(hàm()
    trong khi task.wait() làm
        pcall(hàm()
            nếu _G.BringMonster thì
                Kiểm traQuest()
                đối với i,v theo cặp (game:GetService("Workspace").Enemies:GetChildren()) thì
                    nếu _G.AutoFarm và StartMagnet và v.Name == Mon và (Mon == "Factory Staff" hoặc Mon == "Monkey" hoặc Mon == "Dragon Crew Warrior" hoặc Mon == "Dragon Crew Archer") và v:FindFirstChild("Humanoid") và v:FindFirstChild("HumanoidRootPart") và v.Humanoid.Health > 0 và (v.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 250 thì
                        v.HumanoidRootPart.Size = Vector3.new(150,150,150)
                        v.HumanoidRootPart.CFrame = PosMon
                        v.Humanoid:ChangeState(14)
                        v.HumanoidRootPart.CanCollide = false
                        v.Head.CanCollide = sai
                        nếu v.Humanoid:FindFirstChild("Animator") thì
                            v.Humanoid.Animator:Phá hủy()
                        kết thúc
                        sethiddenproperty(game:GetService("Người chơi").LocalPlayer,"SimulationRadius",math.huge)
                    elseif _G.AutoFarm và StartMagnet và v.Name == Mon và v:FindFirstChild("Humanoid") và v:FindFirstChild("HumanoidRootPart") và v.Humanoid.Health > 0 và (v.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= _G.BringMode thì
                        v.HumanoidRootPart.Size = Vector3.new(150,150,150)
                        v.HumanoidRootPart.CFrame = PosMon
                        v.Humanoid:ChangeState(14)
                        v.HumanoidRootPart.CanCollide = false
                        v.Head.CanCollide = sai
                        nếu v.Humanoid:FindFirstChild("Animator") thì
                            v.Humanoid.Animator:Phá hủy()
