    M:AddButton({ 
        Name = "INCREASE FPS", 
        Callback = function() 
            pcall(function() 
                game:GetService("Lighting").FantasySky:Destroy() 
                local g = game 
                local w = g.Workspace 
                local l = g.Local Lighting 
                t = w.Terrain 
                t.WaterWaveSize = 0 
                t.Water Wave Speed ​​= 0 
                t.Water Reflection = 0 
                t. Water transparency = 0 
                l.GlobalShadows = false 
                l.FogEnd = 9e9 
                l.Lightness = 0 
                settings().Rendering.QualityLevel = "Level01" 
                for i, v in pairs (g:GetDescendants()) then


                    if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then 
                        v.Material = "Plastic" 
                        v. Reflectance = 0 
                    elseif v:IsA("Decal") or v:IsA("Texture") then 
                        v.Transparency = 1 
                    elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then 
                        v.Lifetime = NumberRange.new(0) 
                    elseif v:IsA("Explosion") then 
                        v.Explosion Pressure = 1 
                        v.BlastRadius = 1 
                    elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") or v:IsA("Sparkles") then 
                        v.Enabled = false 
                    otherwise v:IsA("MeshPart") then 
                        v.Material = "Plastic" 
                        v. Reflection = 0 
                        v.TextureID = 10385902758728957 
                    end end 
                for 
                i, e in pair (l:GetChildren()) then do 
                    if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then 
                        e.Enabled = false 
                end 
                    end 
                for i, v in pair (game:GetService("Workspace").Camera:GetDescendants()) then 
                    if v.Name == ("Water;") then 
                        v. Transparency = 1 
                        v.Material = "Plastic" 
                    end end 
                end 
            ) 
          end     
    }) 
