if game.PlaceId == 2753915549 then 
    World1 = true 
elseif game.PlaceId == 4442272183 then 
    World2 = true 
elseif game.PlaceId == 7449423635 then 
    World3 = true 
else 
    game:GetService("Player").LocalPlayer:Kick("Not supported, please wait...") 
end 

