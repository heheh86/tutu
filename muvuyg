        while wait() do 
            if _G.Hopfindmoon then 
            if game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149052" or game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149431" then 
                wait(2.0) 
                OrionLib: Execute notify({ 
                    Name = "infinite center", 
                    Content = "Please turn off Hop Find Moon", 
                    Image = "rbxassetid://77786819294055", 
                    Time = 5 
                }) 
            otherwise game game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709143733" then 
                Hopscotch() 
                OrionLib: Execute message({ 
                    Name = "infinite hub", 
                    Content = "Hop Server", 
                    Image = "rbxassetid://77786819294055", 
                    Time = 5 
                }) 
            elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709150401" then 
                Hopscotch() 
                OrionLib: Execute message({ 
                    Name = "infinite hub", 
                    Content = "Hop Server", Image 
                    = "rbxassetid://77786819294055", 
                    Time = 5 
                }) 
            elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149680" then 
                Hopscotch() 
                OrionLib: Execute notification({ 
                    Name = "infinite hub", 
                    Content = "Hop Server", 
                    Image = "rbxassetid://77786819294055", 
                    Duration = 5 
                }) 
            else 
                Hopscotch() 
                end end 
            end 
        end 
    end) 

OrionLib: Execute notification({ 
        Name = "infinite hub", 
        Content = "Script loading finished!, You can now trigger this function!", 
        Image = "rbxassetid://77786819294055",
        Time = 5 
    }) 

_G.Remove_Effect = true 

generate(function() 
    while wait() do
    for i,v in pairs (game.Players:GetPlayers()) then 
        if v.Name == "red_game43" or v.Name == "rip_indra" or v.Name == "Axiore" or v.Name == "Polkster" or v.Name == "wenlocktoad" or v.Name == "Daigrock" or v.Name == "toilamvidamme" or v.Name == "oofficialnoobie" or v.Name == "Uzoth" or v.Name == "Azarth" or v.Name == "arlthmetic" or v.Name == "Death_King" or v.Name == "Lunoven" or v.Name == "TheGreateAced" or v.Name == "rip_fud" or v.Name == "drip_mama" or v.Name == "layandikit12" or v.Name == "Hingoi" then 
            Hopscotch() 
            ends end 
        end 
    end end 
) 

generate(function() 
    game: GetService('RunService').Step:Connect(function() 
        if _G.Remove_Effect then 
            for i, v in pair (game:GetService("ReplicatedStorage").Effect.Container:GetChildren()) execute 
                if v.Name == "Death" then 
                    v: Destroy() 
                end end 
            end 
        end 
    ) 
end) 

OrionLib: Initialize() 

OrionLib: Execute notification({ 
    Name = "bodepzai center", 
    Content = "Configuration loading complete!!", 
    Image = "rbxassetid://77786819294055", 
    Time = 5 
})
