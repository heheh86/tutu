Local section = S:AddSection({ 
    Name = "Select stats count" 
}) 

generate(function() 
    while wait() do 
        local count10 = 0 
        local count = 0 
        for i,v in pairs (game.workspace:GetChildren()) then do 
            if string.find(v.Name,"Chest") and v:IsA("Part") then 
                count10 = count10 + 1 
            end end 
        for 
        i,v in pairs (game.Workspace:GetChildren()) then do 
            if v.Name == "Blox Fruit Dealer" then 
            else 
                if string.find(v.Name, "Fruit") and v:IsA("Tool") then 
                    count = count + 1 
                end 
                if string.find(v.Name, "Fruit") and v:IsA("Model") then 
                    count = count + 1 
                end end 
            end 
        Fruit 
        :Set("Fruit : "..count) 
        Chest:Set("Chest : "..count10) 
        wait(5) 
    end end 
) 

