        end 
    end) 
    Local Section = C:AddSection({ 
        Name = "Other Players" 
    }) 
    C:AddConvert({ 
        Name = "Infinity Soru", 
        Default = false, 
        Callback = function(Value) 
            getgenv().InfSoru = Value 
        end     
