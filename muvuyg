Local section = LC:AddSection({ 
    Name = "Players" 
}) 

generate(function() 
    while wait() do 
        pcall(function() 
            for i,v in pairs (game:GetService("Players"):GetPlayers()) then 
                if i == 12 then 
                    plyserv:Set("Players:".." "..i.." ".."/".." ".."12".." ".."(Max)") 
                otherwise i == 1 then 
                    plyserv:Set("Players:".." "..i.." ".."/".." ".."12") 
                else 
                    plyserv:Set("Players:".." "..i.." ".."/".." ".."12") 
                end 
            end end 
        ) 
    end 
end) 

Player List = {} 
    
    for i,v in pairs (game:GetService("Players"):GetChildren()) then 
        table.insert(Player List,v.Name)
    end 
    
local SelectedPly = LC:AddDropdown({ 
    Name = "Select Player", 
    Default = "", 
    Options = PlayerList, 
    Callback = function(Value) 
        _G.SelectPly = Value 
    end     
}) 

LC:AddButton({ 
    Name = "Refresh Players", 
    Callback = function() 
        New PlayerList = {} 
    for i,v in pairs (game.Players:GetChildren()) then   
        table.insert(PlayerList, v.Name) 
    end 
    SelectedPly: Refresh(New PlayerList) 
      end      
}) 

