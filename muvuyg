        SV:Add text box({ 
            Name = "Set job code", 
            Default = "", 
            TextDisappear = true,
            Callback = function(Value) _G.Job = 
            End	   
                Value 
        }) 
        SV:Add button({

            Name = "Join ID", 
            Callback = function() 
                _G.Auto Rejoin = false 
                game: GetService("Teleport Service"): Teleport to placeInstance(game.placeId,_G.Job, game.Players.LocalPlayer) 
              end     
        }) 

        SV:Add button({ 
            Name = "Copy Job ID", 
            Callback = function() 
                set clipboard(tostring(game.JobId)) 
              end     
            }) 

        SV:Add button({ 
            Name = "Hop Sever", 
            Callback = function() 
                _G.Auto Rejoin = false 
                Hopscotch() 
              end     
        }) 

        SV:Add button({ 
            Name = "Rejoin Sever", 
            Callback = function() 
                game: GetService("Teleport Service"):Teleport(game.PlaceId, game: GetService("Players").LocalPlayer) 
              end     
        }) 

        SV:Add button({ 
            Name = "Hop Sever Low Players", 
            Callback = function() 
                _G.Auto Rejoin = false 
                getgenv().AutoTeleport = true 
                getgenv().DontTeleportTheSameNumber = true 
                getgenv().CopytoClipboard = false 
                if not game:IsLoaded() then 
                    print("Game is loading, waiting...") 
                end 
                local maxplayers = math.huge 
                local servermaxplayer; 
                local goods server; local 
                gamelink = "https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100" 
                function serversearch() 
                    for _, v in pairs (game: GetService("HttpService"):JSONDecode(game: HttpGetAsync(gamelink)).data) do 
                        if type(v) == "table" and v.playing ~= nil and maxplayers > v.playing then 
                            serversmaxplayer = v.maxPlayers 
                            maxplayers = v.playing 
                            goodserver = v.id 
                        end 
                    end        
                end 
                function getservers() 
                    findservers()
                    for i,v in pairs (game:GetService("HttpService"):JSONDecode(game:HttpGetAsync(gamelink))) do 
                        if i == "nextPageCursor" then 
                            if gamelink:find("&cursor=") then 
                                local a = gamelink:find("&cursor=") local 
                                b = gamelink:sub(a) 
                                gamelink = gamelink:gsub(b, "") 
                            end 
                            gamelink = gamelink .. "&cursor=" ..v 
                            getserver() 
                        end 
                    end 
                end 
                getserver() 
                if Auto-shift then 
                    if not shift same number then 
                        if #game:GetService("Players"):GetPlayers() - 4 == maxplayers then 
                            return warn("Have the same number of players (except you)") 
                        otherwise goodserver == game.JobId then 
                            return warn("Your current server is the most empty server right now") 
                        end 
                    end 
                    game: GetService("Service service move"): Teleport to place Instance(game.PlaceId, goodserver) 
                end end 
              }     
        ) 

