            game.Players.LocalPlayer.Character.Busy.Value = false         
        end end 
    ) 
end) 

Local Attack List = {"0", "0.1", "0.175", "0.2", "0.25", "0.3", "0.35", "0.4", "0.45", "0.5", "0.55", "0.6", "0.65", "0.7", "0.75", "0.8", "0.85", "0.9", "0.95", "0.1"} 
M:AddDropdown({ 
	Name = "FastAttack Delay", 
	Default = "0.5", 
	Options = AttackList,
    Flag = "Fast Attack Delay", 
    Save = true, 
	Callback = function(Value) _G.FastAttackDelay = 
	End     
		Value 
}) 
generate(function()
    while wait(.1) do 
        if _G.FastAttackDelay then 
            pcall(function() 
                if _G.FastAttackDelay == "0" then 
                _G.FastAttackDelay 
                    = 
                    0 otherwise _G.FastAttackDelay == "0.1" then _G.FastAttackDelay = 0.1 
                otherwise _G.FastAttackDelay == "0.175" then 
                    _G.FastAttackDelay 
                = 
                    0.175 
                otherwise _G.FastAttackDelay == "0.2" then _G.FastAttackDelay = 
                    0.25 
                otherwise _G.FastAttackDelay == "0.3" then 
                    _G.FastAttackDelay = 0.3 
                otherwise _G.FastAttackDelay == "0.35" then _G.FastAttackDelay 
                    = 0.35 otherwise _G.FastAttackDelay == "0.4" then _G.FastAttackDelay = 
                    0.5 
                otherwise _G.FastAttackDelay == " 
                0.45 
                    " then _G.FastAttackDelay = 0.45 otherwise _G.FastAttackDelay == "0.5" then _G.FastAttackDelay = 0.5 
                otherwise _G.FastAttackDelay == "0.55" then 
                _G.FastAttackDelay 
                    = 0.55 
                    otherwise 
                _G.FastAttackDelay == "0.6" then 
                    _G.FastAttackDelay = 0.6 
                otherwise _G.FastAttackDelay == "0.65" then 
                    _G.FastAttackDelay = 0.65 
                otherwise _G.FastAttackDelay == "0.7" then _G.FastAttackDelay 
                    = 0.7 otherwise _G.FastAttackDelay 
                == "0.75" then _G.FastAttackDelay = 0.75 otherwise _G.FastAttackDelay == "0.8" then _G.FastAttackDelay = 0.85 otherwise _G.FastAttackDelay == " 
                    0.85 
                " then 
                    _G.FastAttackDelay 
                = 
                    0.85 
                otherwise _G.FastAttackDelay == "0.9" then _G.FastAttackDelay 
                    = 0.95 
                otherwise _G.FastAttackDelay == "0.95" then 
                    _G.FastAttackDelay = 0.95 
                if otherwise _G.FastAttackDelay == "1" then 
                    _G.FastAttackDelay = 1 
                end end 
            ) 
        end 
    end 
end) 
