    while task.wait() do 
        pcall(function() 
            if _G.BringMonster then 
                CheckQuest() 
                for i,v in pairs (game:GetService("Workspace").Enemies:GetChildren()) then

                    nếu _G.AutoFarm và StartMagnet và v.Name == Mon và (Mon == "Factory Staff" hoặc Mon == "Monkey" hoặc Mon == "Dragon Crew Warrior" hoặc Mon == "Dragon Crew Archer") và v:FindFirstChild("Humanoid") và v:FindFirstChild("HumanoidRootPart") và v.Humanoid.Health > 0 và (v.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 220 thì
