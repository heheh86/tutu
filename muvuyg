D:AddSwitch({ 
    Name = "Auto-save fruits", 
    Default = false, 
    Flag = "Auto Store Fruit", 
    Save = true, 
    Callback = function(Value) _G.AutoStoreFruit = 
    End     
        Value 
}) 
function DropFruit() 
    pcall(function() 
        game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Position = UDim2.new(10.100, 0, 0.100, 0) -- HideUi 
        game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Visible = true -- 
        Local invenfruit = game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Container.Stored.ScrollingFrame.Frame 
        wait(.3) 
        for i,v in pairs (invenfruit:GetChildren()) then do 
            if string.find(v.Name,"-") then 
                for _,Backpack in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) then 
                    FruitStoreF = string.split(Backpack.Name, " ")[1] 
                    FruitStoreR = FruitStoreF.."-"..FruitStoreF 
                    if v.Name == FruitStoreR then 
                        game: GetService("Players").LocalPlayer.Backpack:FindFirstChild(Backpack.Name):Destroy() 
                    end												 
                end 
            end 
        end 
        for i,v in pairs (invenfruit:GetChildren()) then do 
            if string.find(v.Name,"-") then 
                for _,Characters in pairs (game:GetService("Player").LocalPlayer.Character:GetChildren()) executes 
                    FruitStoreF = string.split(Character.Name, " ")[1] 
                    FruitStoreR = FruitStoreF.."-"..FruitStoreF 
                    if v.Name == FruitStoreR then

                        game: GetService("Player").LocalPlayer.Character:FindFirstChild(Character.Name):Destroy() 
                    end end												 
                end 
            end 
        end 
    end) 
end 
D:AddTransition({ 
    Name = "Teleport to Fruit Spawn", 
    Default = false, 
    Flag = "Teleport to fruit spawn", 
    Save = true, 
    Callback = function(Value) 
        _G.Tweenfruit = Value 
        StopTween(_G.Tweenfruit) 
    end     
}) 
spawn(function() 
    while wait(.1) do 
        if _G.Tweenfruit then 
            for i,v in pairs (game.Workspace:GetChildren()) then do 
                if string.find(v.Name, "Fruit") then 
                    topos(v.Handle.CFrame) 
                end end 
            end 
        end 
	end 
end) 

D:AddTransition({ 
    Name = "Auto-drop fruit", 
    Default = false, 
    Flag = "Auto-drop fruit", 
    Save = true, 
    Callback = function(Value) 
        _G.DropFruit = Value 
    end     
}) 
generate(function() 
    while wait() do 
        if _G.DropFruit then 
            pcall(function() 
                for i,v in pairs (game:GetService("Players").LocalPlayer.Backpack:GetChildren()) then 
                    if string.find(v.Name, "Fruit") then 
                        EquipWeapon(v.name) 
                        wait(.1) 
                        if game:GetService("Players").LocalPlayer.PlayerGui.Main.Dialogue.Visible == true then 
                            game:GetService("Players").LocalPlayer.PlayerGui.Main.Dialogue.Visible = false 
                        end 
                        EquipWeapon(v.name) 
                        game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectFruit).EatRemote:InvokeServer("Drop") 
                    end end 
                for 
            i,v in pairs (game:GetService("Players").LocalPlayer.Character:GetChildren()) then 
                    if string.find(v.Name, "Fruit") then 
                        Equip weapon(v.name) 
                        wait(.1) 
                        if game:GetService("Players").LocalPlayer.PlayerGui.Main.Dialogue.Visible == true thì
                            game: GetService("Player").LocalPlayer.PlayerGui.Main.Dialogue.Visible = false 
                        end 
                        Equip Weapon(v. name) 
                        game: GetService("Player").LocalPlayer.Character:FindFirstChild(SelectFruit).EatRemote:InvokeServer("Drop") 
                    end 
                end end 
            ) 
        end end ) 
    end 
end) 

