        while wait() do 
            if _G.AutoVampire and World2 then 
                pcall(function() 
                    if game: GetService("Workspace").Enemies:FindFirstChild("Vampire") then 
                        for i,v in pairs (game:GetService("Workspace").Enemies:GetChildren()) then 
                            if v.Name == "Vampire" then 
                                if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then 
                                    repeat the task.wait() 
                                        Auto Haki() 
