    function SaveSettings(m, n) 
        if m ~= nil then 
            time[m] = n 
        end 
        HttpService = game:GetService("HttpService") 
        if not isfolder(i) then 
            makefolder(i) 
        end 
        writefile(i .. "/" .. l, Http Service:JSONEncode(h)) 
    end 
    function ReadSetting() 
        local s, o = 
            call( 
            function() 
                HttpService = game:GetService("HttpService") 
                if not isfolder(i) then 
                    makefolder(i) 
                end 
                return HttpService:JSONDecode(readfile(i .. "/" .. l)) 
            end 
        ) 
        if s then 
            return o 
        else 
            SaveSettings() 
            return ReadSettings() 
        end 
    end 

    function MoonTextureId() 
        if Sea1 then 
            return game: GetService("Lighting").FantasySky.MoonTextureId 
        otherwise Sea2 then 
            return game:GetService("Lighting").FantasySky.MoonTextureId
        elseif Sea3 thì
            return game: GetService("Lighting").Sky.MoonTextureId 
        end 
    end 

    function CheckMoon() 
        moon8 = "http://www.roblox.com/asset/?id=9709150401" 
        moon7 = "http://www.roblox.com/asset/?id=9709150086" 
        moon6 = "http://www.roblox.com/asset/?id=9709149680" 
        moon5 = "http://www.roblox.com/asset/?id=9709149431" 
        moon4 = "http://www.roblox.com/asset/?id=9709149052" 
        moon3 = "http://www.roblox.com/asset/?id=9709143733" 
        moon2 = "http://www.roblox.com/asset/?id=9709139597" 
        moon1 = "http://www.roblox.com/asset/?id=9709135895" 
        moonreal = MoonTextureId() 
        cofullmoonkothangbeo = "Bad moon" 
        if moonreal == moon5 or moonreal == moon4 then 
            if moonreal == moon5 then 
                cofullmoonkothangbeo = "Full moon" 
            otherwise moonreal == moon4 then 
                cofullmoonkothangbeo = "Next night" 
            end 
        end 
        return cofullmoonkothangbeo 
    end 

    function function7() 
        GameTime = "Error" 
        local c = game.Lighting local 
        ao = c.ClockTime 
        if ao >= 18 or ao < 5 then 
            GameTime = "Night" 
        else 
            GameTime = "Day" 
        end 
        return GameTime 
    end 
    function function6() 
        return math.floor(game.Lighting.ClockTime) 
    end 
    function getServerTime() 
        RealTime = tostring(math.floor(game.Lighting.ClockTime * 100) / 100) 
        RealTime = tostring(game.Lighting.ClockTime) 
        RealTimeTable = RealTime:divide(".") 
        Minutes, Seconds = RealTimeTable[1], tonumber(0 + tonumber(RealTimeTable[2] / 100)) * 60 
        return Minutes, Seconds 
    end 
    function function8() 
        local c = game.Light 
        local ao = c.ClockTime 
        if CheckMoon() == "Full Moon" and ao <= 5 then 
            return string(function6()) .. " ( Will end Moon in " .. math.floor(5 - ao) .. " Minutes )" 
        elseif CheckMoon() == "Full Moon" and (ao > 5 and ao < 12) then 
            return string(function6()) .." ( Fake Moon )" 
        elseif CheckMoon() == "Full Moon" and (ao > 12 and ao < 18) then 
            return string(function6()) .. " ( Full Moon will be in " .. math.floor(18 - ao) .. " Minutes )"
        elseif CheckMoon() == "Full Moon" and (ao > 18 and ao <= 24) then 
            return string(function6()) .. " ( Will end Moon in " .. math.floor(24 + 6 - ao) .. 
            " Minutes ) " 
        end 
        if CheckMoon() == "Next Night" and ao < 12 then 
        return 
            string(function6()) .. " ( Full Moon will be in " .. math.floor(18 - ao) .. " Minutes )" 
        end 
        return string(function6()) 
    end 
    CheckAcientOneStatus() function 
        if not game.Players.LocalPlayer.Character:FindFirstChild("RaceTransformed") then 
            return "You have not achieved greatness yet" 
        end 
        local v227 = nil 
        local v228 = nil 
        local v229 = nil 
        v229, v228, v227 = game.ReplicatedStorage.Remotes.CommF_:InvokeServer("UpgradeRace", "Check") 
        if v229 == 1 then 
            return "Request additional training" 
        elseif v229 == 2 or v229 == 4 or v229 == 7 then 
            return "Equipment can be purchased with " .. v227 .. " Fragments " 
        otherwise v229 == 3 then 
            return "Request additional training" 
        otherwise v229 == 5 then 
            return "You have completed your race." 
        otherwise v229 == 6 then 
            return "Upgrades completed: " .. v228 - 2 .. "/3, Need more trains" 
        end 
        if v229 ~= 8 then 
            if v229 == 0 then 
                return "Ready for testing" 
            else 
                return "You haven't achieved greatness yet" 
            end end 
        return 
        "Remaining" .. 10 - v228 .. "training sessions." 
    end 
spawn(function() 
    while wait() do 
        for i,v in pairs (game.Players.LocalPlayer:GetChildren()) then 
            if v.Name == "DataLoaded" or v.Name == "DataPreloaded" then 
                v: Destroy() 
            end 
        end 
    end 
end) 
